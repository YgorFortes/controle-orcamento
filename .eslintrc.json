{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb-typescript/base"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off",
    "import/no-extraneous-dependencies": "off",
    "no-console": "off",
    "typescript-eslint/return-await": "off"
  },
  "ignorePatterns": [
    "jest.config.ts",
    "prisma-enviroment-jest.js",
    "revenue.test.ts", "expanse.test.ts", "summary.test.ts", "user.test.ts", "auth-user.test.ts"
  ]
}
